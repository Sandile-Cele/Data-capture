@model IEnumerable<Data_capture.Models.MeasurementMetaDatum>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MmdTimeStamp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MmdStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MIdNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MmdTimeStamp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MmdStatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MIdNavigation.UserId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MmdId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MmdId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MmdId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
